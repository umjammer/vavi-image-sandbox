<?xml version="1.0" encoding="UTF-8"?>

<project
    xmlns="http://maven.apache.org/POM/4.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>vavi</groupId>
  <artifactId>vavi-image-sandbox</artifactId>
  <name>Vavi Imaging Sandbox API</name>
  <organization>
    <name></name>
  </organization>
  <version>1.0.0</version>
  <url>http://www.vavisoft.com/</url>
  <description></description>
  <build>
    <plugins>
      <plugin>
        <artifactId>maven-compiler-plugin</artifactId>
        <configuration>
          <source>6</source>
          <target>6</target>
          <encoding>UTF-8</encoding>
          <compilerVersion>6</compilerVersion>
          <executable>${JAVA_HOME}/bin/java</executable>
        </configuration>
      </plugin>
    </plugins>
  </build>
  <repositories>
  </repositories>
  <dependencies>
  	<dependency>
  		<groupId>vavi</groupId>
  		<artifactId>vavi-commons</artifactId>
  		<version>1.0.0</version>
  		<optional>false</optional>
  	</dependency>
    <dependency>
        <groupId>net.sf.barcode4j</groupId>
        <artifactId>barcode4j</artifactId>
        <version>2.0</version>
        <optional>false</optional>
    </dependency>
    <dependency>
        <groupId>avalon-framework</groupId>
        <artifactId>avalon-framework</artifactId>
        <version>4.1.4</version>
        <optional>false</optional>
    </dependency>
    <dependency>
    	<groupId>vavi</groupId>
    	<artifactId>vavi-image</artifactId>
    	<version>1.0.0</version>
    	<optional>false</optional>
    </dependency>
    <dependency>
    	<groupId>vavi</groupId>
    	<artifactId>vavi-awt</artifactId>
    	<version>1.0.0</version>
    	<optional>false</optional>
    </dependency>
    <dependency>
    	<groupId>commons-logging</groupId>
    	<artifactId>commons-logging</artifactId>
    	<version>1.1.1</version>
    	<optional>false</optional>
    </dependency>
    <dependency>
    	<groupId>commons-collections</groupId>
    	<artifactId>commons-collections</artifactId>
    	<version>3.2</version>
    	<optional>false</optional>
    </dependency>
    <dependency>
    	<groupId>com.mortennobel</groupId>
    	<artifactId>imagescaling</artifactId>
    	<version>0.7.1</version>
    	<optional>false</optional>
    </dependency>
    <dependency>
    	<groupId>junit</groupId>
    	<artifactId>junit</artifactId>
    	<version>4.5</version>
    	<scope>test</scope>
    	<optional>false</optional>
    </dependency>
    <dependency>
    	<groupId>com.sun.media</groupId>
    	<artifactId>jai_imageio</artifactId>
    	<version>1.2</version>
    	<optional>false</optional>
    </dependency>
    <dependency>
    	<groupId>com.sun.jna</groupId>
    	<artifactId>jna</artifactId>
    	<version>3.1.0</version>
    	<optional>false</optional>
    </dependency>
    <dependency>
    	<groupId>vavi</groupId>
    	<artifactId>vavi-commons-sandbox</artifactId>
    	<version>0.3.1</version>
    	<scope>test</scope>
    	<optional>false</optional>
    </dependency>
    <dependency>
        <groupId>com.sixlegs</groupId>
        <artifactId>javapng</artifactId>
        <version>2.0.0</version>
        <optional>false</optional>
    </dependency>
    <dependency>
        <groupId>com.sixlegs</groupId>
        <artifactId>javapng-iio</artifactId>
        <version>2.0.0</version>
        <optional>false</optional>
    </dependency>
  </dependencies>
</project>